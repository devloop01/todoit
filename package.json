{
	"type": "module",
	"name": "todoit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "pnpm check && vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"format": "prettier  --write .",
		"db:start": "supabase start",
		"db:stop": "supabase stop --backup",
		"db:status": "supabase status",
		"db:generate": "drizzle-kit generate:pg",
		"db:push": "tsx ./src/lib/server/db/migrate.ts",
		"db:migrate": "pnpm db:generate && pnpm db:push",
		"db:studio": "drizzle-kit studio --port 3000 --verbose"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.15",
		"@fontsource-variable/open-sans": "^5.0.18",
		"@formkit/auto-animate": "^0.8.1",
		"@lucia-auth/adapter-postgresql": "^2.0.2",
		"bits-ui": "^0.9.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.29.0",
		"drizzle-zod": "^0.5.1",
		"lucia": "^2.7.4",
		"lucide-svelte": "^0.292.0",
		"nanoid": "^5.0.3",
		"pg": "^8.11.3",
		"postgres": "^3.4.3",
		"resend": "^2.0.0",
		"svelte-email": "^0.0.4",
		"tailwind-merge": "^2.0.0",
		"tailwindcss-animate": "^1.0.7"
	},
	"devDependencies": {
		"@melt-ui/pp": "^0.1.4",
		"@melt-ui/svelte": "^0.61.2",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/kit": "^1.27.6",
		"@types/node": "^20.9.0",
		"@typescript-eslint/eslint-plugin": "^6.11.0",
		"@typescript-eslint/parser": "^6.11.0",
		"autoprefixer": "^10.4.16",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.20.2",
		"eslint": "^8.53.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.0",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"supabase": "^1.112.0",
		"svelte": "^4.2.3",
		"svelte-check": "^3.6.0",
		"svelte-sequential-preprocessor": "^2.0.1",
		"sveltekit-flash-message": "^2.2.2",
		"sveltekit-superforms": "^1.10.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"tsx": "^4.1.2",
		"typescript": "^5.2.2",
		"vite": "^4.5.0",
		"zod": "^3.22.4"
	}
}
