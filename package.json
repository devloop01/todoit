{
	"type": "module",
	"name": "todoit",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"db:start": "supabase start",
		"db:stop": "supabase stop --backup",
		"db:status": "supabase status",
		"db:generate": "drizzle-kit generate:pg",
		"db:push": "tsx ./src/lib/server/db/migrate.ts",
		"db:migrate": "pnpm db:generate && pnpm db:push",
		"db:studio": "drizzle-kit studio --port 3000 --verbose"
	},
	"dependencies": {
		"@fontsource-variable/inter": "^5.0.8",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.0.0",
		"drizzle-orm": "^0.28.2",
		"lucide-svelte": "^0.265.0",
		"pg": "^8.11.2",
		"postgres": "^3.3.5",
		"tailwind-merge": "^1.14.0",
		"tailwindcss-animate": "^1.0.6"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.22.5",
		"@typescript-eslint/eslint-plugin": "^6.3.0",
		"@typescript-eslint/parser": "^6.3.0",
		"autoprefixer": "^10.4.14",
		"dotenv": "^16.3.1",
		"drizzle-kit": "^0.19.12",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.32.4",
		"postcss": "^8.4.27",
		"prettier": "^3.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.2",
		"supabase": "^1.83.7",
		"svelte": "^4.2.0",
		"svelte-check": "^3.5.0",
		"sveltekit-superforms": "^1.5.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"tsx": "^3.12.7",
		"typescript": "^5.1.6",
		"vite": "^4.4.9",
		"zod": "^3.21.4"
	}
}
